{
  "name": "react-kit-ts",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "node bin/run build -e production",
    "commit": "git-cz",
    "lint": "tslint .",
    "lint:fix": "npm run lint -- --fix",
    "start": "node bin/run start",
    "test": "node bin/run test -e test",
    "test:report": "npm test && codecov"
  },
  "author": "Colin Dekker",
  "repository": {
    "url": "https://github.com/colindekker/react-kit.git"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "normalize.css": "7.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "3.1.3",
    "react-loadable": "5.3.1",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-saga": "^1.0.0-beta.0",
    "reselect": "3.0.1"
  },
  "devDependencies": {
    "@types/clean-webpack-plugin": "^0.1.0",
    "@types/copy-webpack-plugin": "^4.0.1",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/html-webpack-plugin": "^2.30.1",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/lodash-webpack-plugin": "^0.11.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.0",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-hot-loader": "3.0.5",
    "@types/react-loadable": "5.3.2",
    "@types/react-redux": "^5.0.14",
    "@types/react-router": "^4.0.21",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-router-redux": "^5.0.11",
    "@types/redux-actions": "^2.2.3",
    "@types/redux-devtools": "^3.0.42",
    "@types/redux-devtools-dock-monitor": "^1.1.31",
    "@types/redux-devtools-log-monitor": "^1.0.32",
    "@types/webpack": "^3.8.4",
    "@types/webpack-bundle-analyzer": "^2.9.0",
    "@types/webpack-dev-middleware": "^2.0.0",
    "@types/webpack-env": "^1.13.5",
    "@types/webpack-hot-middleware": "^2.16.2",
    "@types/webpack-merge": "^4.1.1",
    "autoprefixer": "^7.2.5",
    "awesome-typescript-loader": "^3.4.1",
    "css-loader": "0.28.9",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.3.0",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "2.30.1",
    "jest": "22.1.4",
    "minimist": "1.2.0",
    "node-sass": "4.7.2",
    "postcss-loader": "2.1.0",
    "react-addons-test-utils": "15.6.2",
    "sass-loader": "6.0.6",
    "sinon": "4.2.2",
    "style-loader": "0.20.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.7.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.4.0",
    "typescript": "^2.7.1",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.10.0",
    "webpack-dev-server": "2.11.1"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.js$",
    "roots": [
      "src"
    ],
    "modulePaths": [
      "src"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/config/**",
      "!**/bin/**",
      "!src/index.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/mock/browser.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/mock/file.js",
      "\\.(css|scss)$": "<rootDir>/config/mock/style.js"
    },
    "globals": {
      "__DEV__": false,
      "__TEST__": true,
      "__PROD__": false
    }
  }
}
